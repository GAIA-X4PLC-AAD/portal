gax-validation:WalletShape
        rdf:type        sh:NodeShape;
        sh:targetClass  gax-trust-framework:Wallet .

gax-validation:DataConnectorShape
        rdf:type        sh:NodeShape;
        sh:property     [ skos:example    "'An ML service for training, deploying and improving image classifiers.'";
                          sh:datatype     xsd:string;
                          sh:description  "A description in natural language";
                          sh:maxCount     1;
                          sh:name         "description";
                          sh:order        6;
                          sh:path         dct:description
];
        sh:property     [ skos:example    "'https://gaia-x.eu'";
                          sh:description  "a list of resource with information of who owns the data.";
                          sh:minCount     1;
                          sh:name         "virtual resource";
                          sh:nodeKind     sh:IRI;
                          sh:order        2;
                          sh:path         gax-trust-framework:virtualResource
];
        sh:property     [ skos:example    "'GDPR2016', 'LGPD2019'";
                          sh:datatype     xsd:string;
                          sh:description  "a list of data protection regime from the list available below";
                          sh:name         "data protection regime";
                          sh:order        3;
                          sh:path         gax-trust-framework:dataProtectionRegime
];
        sh:property     [ skos:example    "'https://gaia-x.eu'";
                          sh:datatype     xsd:anyURI;
                          sh:description  "a list of policy expressed using a DSL used by the data connector policy engine leveraging Gaia-X Self-descriptions as both data inputs and policy inputs";
                          sh:minCount     1;
                          sh:name         "policies";
                          sh:order        3;
                          sh:path         gax-trust-framework:policies
];
        sh:property     [ skos:example    "'ML', 'Classification'";
                          sh:datatype     xsd:string;
                          sh:description  "Keywords that describe / tag the service.";
                          sh:name         "keyword";
                          sh:order        7;
                          sh:path         dcat:keyword
];
        sh:property     [ skos:example    "'2021-07-17T00:31:30Z'";
                          sh:datatype     xsd:dateTimeStamp;
                          sh:description  "Timestamp the service has been created.";
                          sh:name         "creation time";
                          sh:order        5;
                          sh:path         gax-trust-framework:creationTime
];
        sh:property     [ skos:example    "'(reference to standard'";
                          sh:description  "Provides information about applied standards.";
                          sh:name         "standard conformity";
                          sh:node         gax-validation:StandardShape;
                          sh:order        6;
                          sh:path         gax-trust-framework:standardConformity
];
        sh:property     [ skos:example    "'https://gaia-x.eu'";
                          sh:description  "a list of resource with information of where the data is located";
                          sh:minCount     1;
                          sh:name         "physical resource";
                          sh:nodeKind     sh:IRI;
                          sh:order        1;
                          sh:path         gax-trust-framework:physicalResource
];
        sh:property     [ skos:example    "'https://gaia-x.eu'";
                          sh:description  "DID of resource self-escription related to the service and that can exist independently of it.";
                          sh:name         "aggregation of";
                          sh:nodeKind     sh:IRI;
                          sh:order        2;
                          sh:path         gax-core:aggregationOf
];
        sh:property     [ skos:example    "'https://gaia-x.eu'";
                          sh:description  "DID of participant self-descrription, who is offering this service offering.";
                          sh:maxCount     1;
                          sh:minCount     1;
                          sh:name         "offered by";
                          sh:nodeKind     sh:IRI;
                          sh:order        1;
                          sh:path         gax-core:offeredBy
];
        sh:property     [ skos:example    "'Hybrid', 'gax:PrivateProvisioning'";
                          sh:datatype     xsd:string;
                          sh:description  "Provision type of the service";
                          sh:maxCount     1;
                          sh:name         "provision type";
                          sh:order        8;
                          sh:path         gax-trust-framework:provisionType
];
        sh:property     [ skos:example    "' '";
                          sh:datatype     xsd:string;
                          sh:description  "a list of policy expressed using a DSL (e.g., Rego or ODRL)";
                          sh:minCount     1;
                          sh:name         "policy";
                          sh:order        2;
                          sh:path         gax-trust-framework:policy
];
        sh:property     [ skos:example    "'dataset', 'container'";
                          sh:datatype     xsd:string;
                          sh:description  "Type of the data asset, which helps discovery.  Preferably a controlled vocabulary entry referenced by URI";
                          sh:name         "type";
                          sh:order        4;
                          sh:path         gax-trust-framework:type
];
        sh:property     [ skos:example    "'https://gaia-x.eu'";
                          sh:description  "DID of the service offering self-description related to the service and that can exist independently of it.";
                          sh:name         "depends on";
                          sh:nodeKind     sh:IRI;
                          sh:order        3;
                          sh:path         gax-core:dependsOn
];
        sh:property     [ skos:example    "' '";
                          sh:description  "a list of methods to export data out of the service";
                          sh:minCount     1;
                          sh:name         "data account export";
                          sh:node         gax-validation:DataAccountExportShape;
                          sh:order        4;
                          sh:path         gax-trust-framework:dataAccountExport
];
        sh:property     [ skos:example    "'https://gaia-x.eu'";
                          sh:description  "a resolvable link to the Terms and Conditions applying to that service.";
                          sh:minCount     1;
                          sh:name         "terms and conditions";
                          sh:node         gax-validation:TermsAndConditionsShape;
                          sh:order        1;
                          sh:path         gax-trust-framework:termsAndConditions
];
        sh:property     [ skos:example    "'Image classification ML service'";
                          sh:datatype     xsd:string;
                          sh:description  "A human readable name of the component";
                          sh:maxCount     1;
                          sh:name         "name";
                          sh:order        5;
                          sh:path         gax-trust-framework:name
];
        sh:property     [ skos:example    "'(reference to endpoint)'";
                          sh:description  "Endpoint through which the Service Offering can be accessed";
                          sh:name         "endpoint";
                          sh:node         gax-validation:EndpointShape;
                          sh:order        9;
                          sh:path         gax-trust-framework:endpoint
];
        sh:targetClass  gax-trust-framework:DataConnector .

gax-validation:CPUShape
        rdf:type        sh:NodeShape;
        sh:property     [ skos:example    "'fpu', 'vme', 'de', 'pse', 'sse', 'sse2', 'ht', 'vmx', 'smx', 'sse4_1', 'sse4_2', 'avx', '3dnowprefetch', 'ibrs_enhanced', 'ept_ad', 'sgx', 'sgx_lc', 'md_clear', 'arch_capabilities', '...'";
