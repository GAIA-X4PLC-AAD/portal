gax-validation:SoftwareOfferingShape
    rdf:type        sh:NodeShape;
    sh:property     [ skos:example    "'ML', 'Classification'";
                      sh:datatype     xsd:string;
                      sh:description  "Keywords that describe / tag the service.";
                      sh:name         "keyword";
                      sh:order        7;
                      sh:path         dcat:keyword
                    ];
    sh:property     [ skos:example    "'Hybrid', 'gax:PrivateProvisioning'";
                      sh:datatype     xsd:string;
                      sh:description  "Provision type of the service";
                      sh:maxCount     1;
                      sh:name         "provision type";
                      sh:order        8;
                      sh:path         gax-trust-framework:provisionType
                    ];
    sh:property     [ skos:example    "'(reference to endpoint)'";
                      sh:description  "Endpoint through which the Service Offering can be accessed";
                      sh:name         "endpoint";
                      sh:node         gax-validation:EndpointShape;
                      sh:order        9;
                      sh:path         gax-trust-framework:endpoint
                    ];
    sh:property     [ skos:example    "'Image classification ML service'";
                      sh:datatype     xsd:string;
                      sh:description  "A human readable name of the component";
                      sh:maxCount     1;
                      sh:name         "name";
                      sh:order        5;
                      sh:path         gax-trust-framework:name
                    ];
    sh:property     [ skos:example    "'https://gaia-x.eu'";
                      sh:description  "a resolvable link to the Terms and Conditions applying to that service.";
                      sh:minCount     1;
                      sh:name         "terms and conditions";
                      sh:node         gax-validation:TermsAndConditionsShape;
                      sh:order        1;
                      sh:path         gax-trust-framework:termsAndConditions
                    ];
    sh:property     [ skos:example    "'An ML service for training, deploying and improving image classifiers.'";
                      sh:datatype     xsd:string;
                      sh:description  "A description in natural language";
                      sh:maxCount     1;
                      sh:name         "description";
                      sh:order        6;
                      sh:path         dct:description
                    ];
    sh:property     [ skos:example    "'https://gaia-x.eu'";
                      sh:description  "DID of participant self-descrription, who is offering this service offering.";
                      sh:maxCount     1;
                      sh:minCount     1;
                      sh:name         "offered by";
                      sh:nodeKind     sh:IRI;
                      sh:order        1;
                      sh:path         gax-core:offeredBy
                    ];
    sh:property     [ skos:example    "'GDPR2016', 'LGPD2019'";
                      sh:datatype     xsd:string;
                      sh:description  "a list of data protection regime from the list available below";
                      sh:name         "data protection regime";
                      sh:order        3;
                      sh:path         gax-trust-framework:dataProtectionRegime
                    ];
    sh:property     [ skos:example    "'https://gaia-x.eu'";
                      sh:description  "DID of resource self-escription related to the service and that can exist independently of it.";
                      sh:name         "aggregation of";
                      sh:nodeKind     sh:IRI;
                      sh:order        2;
                      sh:path         gax-core:aggregationOf
                    ];
    sh:property     [ skos:example    "'https://gaia-x.eu'";
                      sh:description  "DID of the service offering self-description related to the service and that can exist independently of it.";
                      sh:name         "depends on";
                      sh:nodeKind     sh:IRI;
                      sh:order        3;
                      sh:path         gax-core:dependsOn
                    ];
    sh:property     [ skos:example    "' '";
                      sh:datatype     xsd:string;
                      sh:description  "a list of policy expressed using a DSL (e.g., Rego or ODRL)";
                      sh:minCount     1;
                      sh:name         "policy";
                      sh:order        2;
                      sh:path         gax-trust-framework:policy
                    ];
    sh:property     [ skos:example    "' '";
                      sh:description  "a list of methods to export data out of the service";
                      sh:minCount     1;
                      sh:name         "data account export";
                      sh:node         gax-validation:DataAccountExportShape;
                      sh:order        4;
                      sh:path         gax-trust-framework:dataAccountExport
                    ];
    sh:targetClass  gax-trust-framework:SoftwareOffering .

gax-validation:ThirdPartyComplianceCertificateClaimShape
    rdf:type        sh:NodeShape;
    sh:property     [ skos:example    "'https://company-a.com/self-descriptions/cab.jsonld'";
                      sh:description  "Id of the Participant (self-description) endorsed having a Compliance Assessment Body role that claims the compliance";
                      sh:maxCount     1;
                      sh:minCount     1;
                      sh:name         "has compliance assessment body";
                      sh:nodeKind     sh:IRI;
                      sh:order        1;
                      sh:path         gax-trust-framework:hasComplianceAssessmentBody
                    ];
    sh:property     [ skos:example    "'https://company-a.com/self-descriptions/compliance-cert-scheme-iso27001.jsonld'";
                      sh:description  "ID of the Compliance Certification Scheme (self-description) involved in the certification";
                      sh:maxCount     1;
                      sh:name         "has compliance certification scheme";
                      sh:nodeKind     sh:IRI;
                      sh:order        1;
                      sh:path         gax-trust-framework:hasComplianceCertificationScheme
                    ];
    sh:property     [ skos:example    "'https://company-a.com/self-descriptions/service-offering-iaas.jsonld'";
                      sh:description  "ID of the Service Offering (self-description) certified for the compliance";
                      sh:maxCount     1;
                      sh:minCount     1;
                      sh:name         "has service offering";
                      sh:nodeKind     sh:IRI;
                      sh:order        2;
                      sh:path         gax-trust-framework:hasServiceOffering
                    ];
    sh:targetClass  gax-trust-framework:ThirdPartyComplianceCertificateClaim .

gx:ServiceOfferingLabelShape
    rdf:type        sh:NodeShape;
    sh:property     [ sh:description  "Gaia-X Policy Rules and Labelling Criteria for Providers";
                      sh:maxCount     1;
                      sh:minCount     1;
                      sh:name         "Criteria";
                      sh:node         gx:ServiceOfferingCriteriaShape;
                      sh:path         gx:criteria
                    ];
    sh:property     [ sh:description  "a resolvable link to the service offering self-description to which label level is assigned to.";
                      sh:maxCount     1;
                      sh:minCount     1;
                      sh:name         "Assigned to";
                      sh:or           ( [ sh:class  gx:ServiceOffering;
                                          sh:node   gx:ServiceOfferingShape
                                        ]
                                      );
                      sh:path         gx:assignedTo
                    ];
    sh:targetClass  gx:ServiceOfferingLabelLevel1 .

gax-validation:VolumeTypeShape
